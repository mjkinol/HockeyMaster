            <Font name="HockeyFont" size="17.0" url="@../../Fonts/HockeyFont.ttf" />
         </font></Button>
      <Button fx:id="stats" layoutX="159.0" layoutY="447.0" mnemonicParsing="false" onAction="#goStats" prefHeight="35.0" prefWidth="82.0" scaleX="1.5" scaleY="1.5" text="Stats">
      	 <font>
            <Font name="HockeyFont" size="17.0" url="@../../Fonts/HockeyFont.ttf" />
         </font></Button>
      <Button fx:id="signout" layoutX="149.0" layoutY="519.0" mnemonicParsing="false" onAction="#signout" text="Sign out">
         <font>
            <Font name="HockeyFont" size="17.0" url="@../../Fonts/HockeyFont.ttf" />
         </font></Button>
   </children>
</AnchorPane>
