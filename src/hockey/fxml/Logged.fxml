<AnchorPane prefHeight="700.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hockey.java.controller.LoggedController">
   <children>
      
      <Label fx:id="appTitle" alignment="CENTER" layoutX="117.0" layoutY="72.0" prefHeight="27.0" prefWidth="170.0" scaleX="2.0" scaleY="2.0" scaleZ="2.0" text="Hockey Master" textAlignment="CENTER">
         <font>
            <Font name="HockeyFont" size="20.0" url="@../../Fonts/HockeyFont.ttf" />
         </font></Label>
      <Label fx:id="pageTitle" alignment="CENTER" layoutX="152.0" layoutY="132.0" prefHeight="27.0" prefWidth="97.0" scaleX="2.0" scaleY="2.0" scaleZ="2.0" text="Main Menu" textAlignment="CENTER">
         <font>
            <Font name="HockeyFont" size="17.0" url="@../../Fonts/HockeyFont.ttf" />
         </font></Label>
      <Label fx:id="message" layoutX="14.0" layoutY="497.0" prefHeight="34.0" prefWidth="372.0" textAlignment="CENTER" textFill="#c30c0c">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="playLogged" layoutX="144.0" layoutY="354.0" mnemonicParsing="false" onAction="#playLogged" scaleX="2.0" scaleY="2.0" scaleZ="2.0" text="Join Game">
